
{
  "apphosting": [
    {
      "backendId": "ktg-webapp",
      "rootDir": "ktg-webapp",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "apphosting": {
        "source": "nextn"
      }
    }
  ],
  "firestore": {
    "database": "(default)",
    "location": "northamerica-northeast1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "public",
    "ignore": []
  },
  "emulators": {
    "auth": {
      "host": "127.0.0.1",
      "port": 9099
    },
    "firestore": {
      "host": "127.0.0.1",
      "port": 8080
    },
    "ui": {
      "enabled": true,
      "host": "127.0.0.1",
      "port": 4000
    },
    "hosting": {
      "host": "127.0.0.1",
      "port": 5001
    },
    "storage": {
        "host": "127.0.0.1",
        "port": 9199
    },
    "pubsub": {
        "host": "127.0.0.1",
        "port": 8085
    }
  }
}
